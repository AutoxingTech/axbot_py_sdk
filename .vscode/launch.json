                "PYTHONPATH": "/opt/ax/platform/lib/python2.7/dist-packages:/opt/ros/noetic/lib/python2.7/dist-packages",
                "ROS_DISTRO": "noetic",
                "ROS_ETC_DIR": "/opt/ros/noetic/etc/ros",
                "ROS_MASTER_URI": "http://localhost:11311",
                "ROS_PACKAGE_PATH": "/opt/ax/platform/share:/opt/ros/noetic/share",
